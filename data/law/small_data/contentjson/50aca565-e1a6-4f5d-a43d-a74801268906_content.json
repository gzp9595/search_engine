\u544a\u8981\u6c42\u89e3\u9664\u5408\u540c\u7684\u8bc9\u8bbc\u8bf7\u6c42\u672c\u9662\u4ea6\u4e88\u4ee5\u652f\u6301\u3002\u4f9d\u7167\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5408\u540c\u6cd5\u300b\u4e5d\u5341\u56db\u6761\u7b2c\uff08\u4e8c\uff09\u6b3e\u3001\u7b2c\u4e8c\u767e\u516b\u5341\u516d\u6761\u3001\u300a\u6700\u9ad8\u4eba\u6c11\u6cd5\u9662\u5173\u4e8e\u5ba1\u7406\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u7ea0\u7eb7\u6848\u4ef6\u9002\u7528\u6cd5\u5f8b\u95ee\u9898\u7684\u89e3\u91ca\u300b\u7b2c\u4e5d\u6761\u3001\u7b2c\u5341\u6761\u4e4b\u89c4\u5b9a\uff0c\u5224\u51b3\u5982\u4e0b\uff1a\b\u4e00\u3001\u89e3\u9664\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u5de5\u7a0b\u96c6\u56e2\u6709\u9650\u516c\u53f8\u4e0e\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u4e8e2010\u5e7412\u670811\u65e5\u7b7e\u8ba2\u7684\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\uff1b\b\u4e8c\u3001\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u4e8e\u672c\u5224\u51b3\u751f\u6548\u540e\u5341\u65e5\u5185\u5411\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u5de5\u7a0b\u96c6\u56e2\u6709\u9650\u516c\u53f8\u652f\u4ed8\u5de5\u7a0b\u6b3e398\u4e07\u5143\uff1b\b\u4e09\u3001\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u5de5\u7a0b\u96c6\u56e2\u6709\u9650\u516c\u53f8\u5bf9\u5176\u627f\u5efa\u7684\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u6240\u6709\u7684\u4f4d\u4e8e\u6df1\u5733\u5de5\u4e1a\u56ed\u7684\u7efc\u5408\u697c\u3001\u529e\u516c\u697c\u7684\u62cd\u5356\u4ef7\u6b3e\u5728\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u4e0d\u80fd\u5c65\u884c\u4e0a\u8ff0\u7b2c\u4e8c\u9879\u6240\u786e\u8ba4\u7684\u503a\u52a1\u8303\u56f4\u5185\u4f18\u5148\u53d7\u507f\u3002\b\u5982\u679c\u672a\u6309\u672c\u5224\u51b3\u6307\u5b9a\u7684\u671f\u95f4\u5c65\u884c\u7ed9\u4ed8\u91d1\u94b1\u4e49\u52a1\uff0c\u5e94\u5f53\u4f9d\u7167\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u6c11\u4e8b\u8bc9\u8bbc\u6cd5\u300b\u7b2c\u4e8c\u767e\u4e94\u5341\u4e09\u6761\u4e4b\u89c4\u5b9a\uff0c\u52a0\u500d\u652f\u4ed8\u8fdf\u5ef6\u5c65\u884c\u671f\u95f4\u7684\u503a\u52a1\u5229\u606f\u3002\b\u6848\u4ef6\u53d7\u7406\u8d3938640\u5143\uff0c\u51cf\u534a\u6536\u53d619320\u5143\uff0c\u7531\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u8d1f\u62c5\u3002\b\u5982\u4e0d\u670d\u672c\u5224\u51b3\uff0c\u53ef\u5728\u5224\u51b3\u4e66\u9001\u8fbe\u4e4b\u65e5\u8d77\u5341\u4e94\u65e5\u5185\uff0c\u5411\u672c\u9662\u9012\u4ea4\u4e0a\u8bc9\u72b6\uff0c\u5e76\u6309\u5bf9\u65b9\u5f53\u4e8b\u4eba\u7684\u4eba\u6570\u63d0\u51fa\u526f\u672c\uff0c\u4e0a\u8bc9\u4e8e\u6e56\u5317\u7701\u8944\u9633\u5e02\u4e2d\u7ea7\u4eba\u6c11\u6cd5\u9662\u3002\b\u5ba1\u5224\u5458\u3000\u9ec4\u3000\u71d5\b\u4e8c\u3007\u4e00\u516d\u5e74\u5341\u4e8c\u6708\u4e8c\u5341\u4e5d\u65e5\b\u4e66\u8bb0\u5458\u3000\u65b9\u5c0f\u7fe0", "WBWB": "\u5ba1\u5224\u5458\u3000\u9ec4\u3000\u71d5\b\u4e8c\u3007\u4e00\u516d\u5e74\u5341\u4e8c\u6708\u4e8c\u5341\u4e5d\u65e5\b\u4e66\u8bb0\u5458\u3000\u65b9\u5c0f\u7fe0", "DSRXX": "\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u5de5\u7a0b\u96c6\u56e2\u6709\u9650\u516c\u53f8\uff08\u4ee5\u4e0b\u7b80\u79f0\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u516c\u53f8\uff09\u3002\u4f4f\u6240\u5730\uff1a\u9ad8\u90ae\u5e02\u6587\u6e38\u4e2d\u8def138\u53f7\u3002\b\u6cd5\u5b9a\u4ee3\u8868\u4eba\u80e1\u6052\u6625\uff0c\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u516c\u53f8\u8463\u4e8b\u957f\u3002\b\u59d4\u6258\u4ee3\u7406\u4eba\u738b\u5e73\u3001\u674e\u667a\u7fa4\uff0c\u8944\u9633\u5e02\u660e\u6b63\u6cd5\u5f8b\u670d\u52a1\u6240\u6cd5\u5f8b\u5de5\u4f5c\u8005\u3002\u4ee3\u7406\u6743\u9650\uff1a\u7279\u522b\u6388\u6743\uff1a1\u3001\u4ee3\u4e3a\u627f\u8ba4\u3001\u653e\u5f03\u3001\u53d8\u66f4\u8bc9\u8bbc\u8bf7\u6c42\uff1b2\u3001\u8fdb\u884c\u548c\u89e3\u3001\u8c03\u89e3\uff1b3\u3001\u63d0\u8d77\u53cd\u8bc9\uff1b4\u3001\u63d0\u8d77\u4e0a\u8bc9\u7b49\u8bc9\u8bbc\u6743\u5229\uff1b5\u3001\u6267\u884c\u4ee3\u7406\uff1b6\u3001\u4ee3\u9886\u6267\u884c\u6b3e\u7269\uff1b7\u3001\u4ee3\u4e3a\u7b7e\u6536\u6cd5\u5f8b\u6587\u4e66\u3002\b\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\uff08\u4ee5\u4e0b\u7b80\u79f0\u5965\u514b\u6b27\u5e1d\u65af\u79d1\u6280\u516c\u53f8\uff09\u3002\u4f4f\u6240\u5730\uff1a\u8944\u9633\u5e02\u8944\u5dde\u533a\u6df1\u5733\u5de5\u4e1a\u56ed\u5bcc\u5eb7\u4e1c\u8def6\u53f7\u3002\b\u6cd5\u5b9a\u4ee3\u8868\u4eba\u90b9\u667a\u654f\uff0c\u5965\u514b\u6b27\u5e1d\u65af\u79d1\u6280\u516c\u53f8\u8463\u4e8b\u957f\u3002\b\u59d4\u6258\u4ee3\u7406\u4eba\u96f7\u6606\uff0c\u6e56\u5317\u8054\u5e2e\u5f8b\u5e08\u4e8b\u52a1\u6240\u5f8b\u5e08\u3002\u4ee3\u7406\u6743\u9650\uff1a1\u3001\u7b7e\u3001\u6536\u6cd5\u5f8b\u6587\u4e66\uff1b2\u3001\u6709\u6743\u627f\u8ba4\u3001\u653e\u5f03\u3001\u53d8\u66f4\u8bc9\u8bbc\u8bf7\u6c42\uff1b3\u3001\u8c03\u89e3\u3001\u548c\u89e3\u3002", "SSJL": "\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u516c\u53f8\u4e0e\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u79d1\u6280\u516c\u53f8\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u7ea0\u7eb7\u4e00\u6848\uff0c\u672c\u9662\u4e8e2016\u5e7412\u67085\u65e5\u53d7\u7406\u540e\uff0c\u4f9d\u6cd5\u9002\u7528\u7b80\u6613\u7a0b\u5e8f\uff0c\u7531\u5ba1\u5224\u5458\u9ec4\u71d5\u72ec\u4efb\u5ba1\u5224\uff0c\u5b9a\u4e8e2016\u5e7412\u670823\u65e5\u516c\u5f00\u5f00\u5ead\u8fdb\u884c\u5ba1\u7406\u3002\u9001\u8fbe\u5e94\u8bc9\u624b\u7eed\u540e\uff0c\u88ab\u544a\u81ea\u613f\u653e\u5f03\u7b54\u8fa9\u3001\u4e3e\u8bc1\u671f\u9650\uff0c\u7533\u8bf7\u63d0\u524d\u5f00\u5ead\uff0c2016\u5e7412\u67089\u65e5\u672c\u9662\u516c\u5f00\u5f00\u5ead\u5ba1\u7406\u4e86\u672c\u6848\u3002\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u516c\u53f8\u7684\u59d4\u6258\u4ee3\u7406\u4eba\u738b\u5e73\u3001\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u79d1\u6280\u516c\u53f8\u7684\u59d4\u6258\u4ee3\u7406\u4eba\u96f7\u6606\u5230\u5ead\u53c2\u52a0\u8bc9\u8bbc\u3002\u672c\u6848\u73b0\u5df2\u5ba1\u7406\u7ec8\u7ed3\u3002", "PubDate": "2017-04-04", "Title": "\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u5de5\u7a0b\u96c6\u56e2\u6709\u9650\u516c\u53f8\u4e0e\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u516c\u53f8\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u7ea0\u7eb7\u4e00\u6848\u7684\u4e00\u5ba1\u6c11\u4e8b\u5224\u51b3\u4e66", "CPYZ": "\u672c\u9662\u8ba4\u4e3a\uff0c\u539f\u544a\u4f5c\u4e3a\u6709\u5efa\u8bbe\u65bd\u5de5\u8d44\u8d28\u7684\u5355\u4f4d\u4e0e\u88ab\u544a\u7b7e\u8ba2\u7684\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\u662f\u5f53\u4e8b\u4eba\u771f\u5b9e\u610f\u601d\u8868\u793a\uff0c\u5185\u5bb9\u4e0d\u8fdd\u53cd\u6cd5\u5f8b\u3001\u6cd5\u89c4\u7684\u5f3a\u5236\u6027\u89c4\u5b9a\uff0c\u7cfb\u6709\u6548\u5408\u540c\uff0c\u672c\u9662\u4e88\u4ee5\u786e\u8ba4\u3002\u88ab\u544a\u672a\u6309\u65f6\u5411\u539f\u544a\u652f\u4ed8\u5de5\u7a0b\u6b3e\uff0c\u5bfc\u81f4\u5de5\u7a0b\u5ef6\u671f\uff0c\u5e76\u81f3\u4eca\u672a\u80fd\u4ea4\u4ed8\uff0c\u5c5e\u88ab\u544a\u8fdd\u7ea6\uff0c\u88ab\u544a\u5e94\u627f\u62c5\u652f\u4ed8\u5de5\u7a0b\u6b3e\u7684\u4e49\u52a1\u3002\u7ecf\u7ed3\u7b97\uff0c\u88ab\u544a\u8ba4\u53ef\u5c1a\u6b20\u539f\u544a\u5de5\u7a0b\u6b3e398\u4e07\u5143\uff0c\u6545\u539f\u544a\u4e3b\u5f20\u88ab\u544a\u652f\u4ed8\u5de5\u7a0b\u6b3e3980000\u5143\u7684\u8bc9\u8bbc\u8bf7\u6c42\u7406\u7531\u6b63\u5f53\u3001\u8bc1\u636e\u5145\u5206\uff0c\u672c\u9662\u4e88\u4ee5\u652f\u6301\uff1b\u539f\u544a\u8fd8\u4e3b\u5f20\u5176\u5e94\u5bf9\u5176\u627f\u5305\u5de5\u7a0b\u6240\u62cd\u5356\u7684\u4ef7\u6b3e\u4eab\u6709\u4f18\u5148\u53d7\u507f\u6743\uff0c\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5408\u540c\u6cd5\u300b\u7b2c\u4e8c\u767e\u516b\u5341\u516d\u6761\u89c4\u5b9a\uff1a\u53d1\u5305\u4eba\u672a\u6309\u7ea6\u5b9a\u652f\u4ed8\u5de5\u7a0b\u4ef7\u6b3e\u7684\uff0c\u627f\u5305\u4eba\u53ef\u4ee5\u50ac\u544a\u53d1\u5305\u4eba\u5728\u5408\u7406\u671f\u9650\u5185\u652f\u4ed8\u4ef7\u6b3e\u3002\u53d1\u5305\u4eba\u903e\u671f\u4e0d\u652f\u4ed8\u7684\uff0c\u9664\u6309\u7167\u5efa\u8bbe\u5de5\u7a0b\u7684\u6027\u8d28\u4e0d\u5b9c\u6298\u4ef7\u3001\u62cd\u5356\u7684\u4ee5\u5916\uff0c\u627f\u5305\u4eba\u53ef\u4ee5\u4e0e\u53d1\u5305\u4eba\u534f\u8bae\u5c06\u8be5\u5de5\u7a0b\u6298\u4ef7\uff0c\u4e5f\u53ef\u4ee5\u7533\u8bf7\u4eba\u6c11\u6cd5\u9662\u5c06\u8be5\u5de5\u7a0b\u4f9d\u6cd5\u62cd\u5356\u3002\u5efa\u8bbe\u5de5\u7a0b\u7684\u4ef7\u6b3e\u5c31\u8be5\u5de5\u7a0b\u6298\u4ef7\u6216\u8005\u62cd\u5356\u7684\u4ef7\u6b3e\u4f18\u5148\u53d7\u507f\u3002\u300a\u6700\u9ad8\u4eba\u6c11\u6cd5\u9662\u5173\u4e8e\u5efa\u8bbe\u5de5\u7a0b\u4ef7\u6b3e\u4f18\u5148\u53d7\u507f\u6743\u95ee\u9898\u7684\u6279\u590d\u300b\u7b2c\u4e00\u6761\u89c4\u5b9a\uff1a\u4eba\u6c11\u6cd5\u9662\u5728\u5ba1\u7406\u623f\u5730\u4ea7\u7ea0\u7eb7\u6848\u4ef6\u548c\u529e\u7406\u6267\u884c\u6848\u4ef6\u4e2d\uff0c\u5e94\u5f53\u4f9d\u7167\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5408\u540c\u6cd5\u300b\u7b2c\u4e8c\u767e\u516b\u5341\u516d\u6761\u7684\u89c4\u5b9a\uff0c\u8ba4\u5b9a\u5efa\u7b51\u5de5\u7a0b\u7684\u627f\u5305\u4eba\u7684\u4f18\u5148\u53d7\u507f\u6743\u4f18\u4e8e\u62b5\u62bc\u6743\u548c\u5176\u4ed6\u503a\u6743\uff1b\u7b2c\u56db\u6761\u89c4\u5b9a\uff1a\u5efa\u8bbe\u5de5\u7a0b\u627f\u5305\u4eba\u884c\u4f7f\u4f18\u5148\u6743\u7684\u671f\u9650\u4e3a\u516d\u4e2a\u6708\uff0c\u81ea\u5efa\u8bbe\u5de5\u7a0b\u7ae3\u5de5\u4e4b\u65e5\u6216\u8005\u5efa\u8bbe\u5de5\u7a0b\u5408\u540c\u7ea6\u5b9a\u7684\u7ae3\u5de5\u4e4b\u65e5\u8d77\u8ba1\u7b97\u3002\u672c\u6848\u8bbc\u4e89\u5efa\u8bbe\u5de5\u7a0b\u81f3\u4eca\u672a\u7ae3\u5de5\uff0c\u4e14\u88ab\u544a\u4e5f\u660e\u786e\u8868\u793a\u5176\u6ca1\u6709\u80fd\u529b\u518d\u652f\u4ed8\u5de5\u7a0b\u6b3e\uff0c\u540c\u610f\u89e3\u9664\u53cc\u65b9\u7b7e\u8ba2\u7684\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\uff0c\u800c\u8bbc\u4e89\u5de5\u7a0b\u622a\u6b622016\u5e7411\u670828\u65e5\u624d\u529e\u7406\u5df2\u5b8c\u5de5\u5de5\u7a0b\u4ef7\u6b3e\u7684\u7ed3\u7b97\uff0c\u8bbc\u4e89\u5de5\u7a0b\u4e5f\u88ab\u62cd\u5356\uff0c\u539f\u544a\u4e3b\u5f20\u5bf9\u62cd\u5356\u4ef7\u6b3e\u4f18\u5148\u53d7\u507f\u5e76\u65e0\u4e0d\u5f53\uff0c\u8be5\u8bc9\u8bbc\u8bf7\u6c42\u672c\u9662\u4e88\u4ee5\u652f\u6301\uff1b\u56e0\u88ab\u544a\u65e0\u80fd\u529b\u518d\u652f\u4ed8\u5de5\u7a0b\u4ef7\u6b3e\uff0c\u53cc\u65b9\u7b7e\u8ba2\u7684\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\u5df2\u65e0\u5c65\u884c\u7684\u53ef\u80fd\uff0c\u4e14\u88ab\u544a\u4e5f\u540c\u610f\u89e3\u9664\u53cc\u65b9\u7b7e\u8ba2\u7684\u65bd\u5de5\u5408\u540c\uff0c\u6545\u539f\u544a\u8981\u6c42\u89e3\u9664\u5408\u540c\u7684\u8bc9\u8bbc\u8bf7\u6c42\u672c\u9662\u4ea6\u4e88\u4ee5\u652f\u6301\u3002\u4f9d\u7167\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5408\u540c\u6cd5\u300b\u4e5d\u5341\u56db\u6761\u7b2c\uff08\u4e8c\uff09\u6b3e\u3001\u7b2c\u4e8c\u767e\u516b\u5341\u516d\u6761\u3001\u300a\u6700\u9ad8\u4eba\u6c11\u6cd5\u9662\u5173\u4e8e\u5ba1\u7406\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u7ea0\u7eb7\u6848\u4ef6\u9002\u7528\u6cd5\u5f8b\u95ee\u9898\u7684\u89e3\u91ca\u300b\u7b2c\u4e5d\u6761\u3001\u7b2c\u5341\u6761\u4e4b\u89c4\u5b9a\uff0c\u5224\u51b3\u5982\u4e0b\uff1a", "WBSB": "\u8944\u9633\u5e02\u8944\u5dde\u533a\u4eba\u6c11\u6cd5\u9662\b\u6c11 \u4e8b \u5224 \u51b3 \u4e66\b\uff082016\uff09\u91020607\u6c11\u521d4247\u53f7", "AJJBQK": "\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u516c\u53f8\u8bc9\u79f0\uff0c2010\u5e7412\u670811\u65e5\uff0c\u539f\u3001\u88ab\u544a\u7b7e\u8ba2\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u4e00\u4efd\u7ea6\u5b9a\uff0c\u7531\u539f\u544a\u627f\u5efa\u88ab\u544a\u7684\u7efc\u5408\u697c\u3001\u529e\u516c\u697c\uff0c\u5de5\u7a0b\u603b\u4ef7\u6b3e958\u4e07\u5143\u3002\u5408\u540c\u7b7e\u8ba2\u540e\uff0c\u539f\u544a\u4f9d\u7ea6\u8fdb\u573a\u65bd\u5de5\uff0c\u56e0\u88ab\u544a\u672a\u6309\u5408\u540c\u7ea6\u5b9a\u6309\u65bd\u5de5\u8fdb\u5ea6\u652f\u4ed8\u5de5\u7a0b\u6b3e\uff0c\u81f4\u4f7f\u5de5\u7a0b\u4e00\u5ea6\u505c\u5de5\uff0c\u539f\u544a\u62df\u7b49\u5f85\u88ab\u544a\u652f\u4ed8\u540e\u7eed\u5de5\u7a0b\u4ef7\u6b3e\u540e\u518d\u884c\u65bd\u5de5\u3002\u540e\u56e0\u88ab\u544a\u516c\u53f8\u4e0e\u4ed6\u4eba\u53d1\u751f\u7ecf\u6d4e\u7ea0\u7eb7\uff0c\u5bfc\u81f4\u539f\u544a\u5df2\u5b8c\u6210\u7684\u5de5\u7a0b\u968f\u88ab\u544a\u516c\u53f8\u6240\u6709\u7684\u571f\u5730\u4f7f\u7528\u6743\u4e00\u8d77\u88ab\u8944\u5dde\u6cd5\u9662\u62cd\u5356\u3002\u539f\u544a\u4ee5\u672c\u516c\u53f8\u5bf9\u8bbc\u4e89\u5de5\u7a0b\u62cd\u5356\u4ef7\u6b3e\u4eab\u6709\u4f18\u5148\u53d7\u507f\u6743\u4e3a\u7531\uff0c\u5411\u8944\u5dde\u6cd5\u9662\u63d0\u51fa\u4e66\u9762\u5f02\u8bae\uff0c\u88ab\u544a\u77e5\u5e94\u7531\u751f\u6548\u6cd5\u5f8b\u6587\u4e66\u8fdb\u884c\u786e\u8ba4\u4f18\u5148\u53d7\u507f\u6743\u3002\u539f\u3001\u88ab\u544a\u7ecf\u534f\u5546\uff0c\u4e8e2016\u5e7411\u670828\u65e5\u5bf9\u5df2\u5b8c\u6210\u7684\u5de5\u7a0b\u7684\u5de5\u7a0b\u91cf\u53ca\u5de5\u7a0b\u4ef7\u6b3e\u8fdb\u884c\u7ed3\u7b97\uff0c\u88ab\u544a\u53ea\u8ba4\u53ef\u5c1a\u6b20\u539f\u544a\u5de5\u7a0b\u6b3e398\u4e07\u5143\uff0c\u56e0\u88ab\u544a\u65e0\u529b\u652f\u4ed8\u8be5\u5de5\u7a0b\u6b3e\uff0c\u4e14\u672c\u6848\u65bd\u5de5\u5408\u540c\u4ea6\u65e0\u5c65\u884c\u7684\u53ef\u80fd\uff0c\u6545\u8bf7\u6c42\u4f9d\u6cd5\u5224\u4ee4\uff1a1\u3001\u89e3\u9664\u539f\u3001\u88ab\u544a\u53cc\u65b9\u7b7e\u8ba2\u7684\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\uff1b2\u3001\u88ab\u544a\u652f\u4ed8\u5de5\u7a0b\u6b3e398\u4e07\u5143\uff1b3\u3001\u786e\u8ba4\u539f\u544a\u5bf9\u5176\u65bd\u5de5\u5b8c\u6210\u7684\u88ab\u544a\u7684\u7efc\u5408\u697c\u3001\u529e\u516c\u697c\u7684\u62cd\u5356\u4ef7\u6b3e\u4eab\u6709\u4f18\u5148\u53d7\u507f\u6743\uff1b4\u3001\u88ab\u544a\u627f\u62c5\u672c\u6848\u8bc9\u8bbc\u8d39\u7528\u3002\b\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u79d1\u6280\u516c\u53f8\u8fa9\u79f0\uff0c1\u3001\u540c\u610f\u89e3\u9664\u539f\u3001\u88ab\u544a\u53cc\u65b9\u7b7e\u8ba2\u7684\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\uff1b2\u3001\u6b20\u5de5\u7a0b\u6b3e398\u4e07\u5143\u5c5e\u5b9e\uff0c\u88ab\u544a\u6682\u65e0\u529b\u507f\u8fd8\uff1b3\u3001\u539f\u544a\u5e94\u5f53\u5bf9\u8bbc\u4e89\u5de5\u7a0b\u7684\u6298\u4ef7\u6216\u62cd\u5356\u540e\u7684\u6b3e\u9879\u4eab\u6709\u4f18\u5148\u53d7\u507f\u6743\u3002\b\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u516c\u53f8\u4e3a\u652f\u6301\u5176\u4e3b\u5f20\uff0c\u5411\u6cd5\u5ead\u4e3e\u51fa\u5982\u4e0b\u8bc1\u636e\uff1a\b\u8bc1\u636e\u4e00\u3001\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\u3001\u5efa\u7b51\u5de5\u7a0b\u9020\u4ef7\u8ba1\u7b97\u4e66\u3001\u5173\u4e8e\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u529e\u516c\u697c\u53ca\u7efc\u5408\u697c\u6240\u6b20\u5de5\u7a0b\u6b3e\u7684\u57fa\u672c\u60c5\u51b5\u3001\u6e05\u7b97\u8bc1\u660e\u3002\u7528\u4ee5\u8bc1\u660e\u539f\u3001\u88ab\u544a\u7b7e\u8ba2\u4e86\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\u3001\u5408\u540c\u7ea6\u5b9a\u7684\u6743\u5229\u4e49\u52a1\u3001\u539f\u544a\u5df2\u5b8c\u6210\u7684\u5de5\u7a0b\u91cf\u3001\u53cc\u65b9\u7ed3\u7b97\u60c5\u51b5\u53ca\u88ab\u544a\u8ba4\u53ef\u6240\u6b20\u539f\u544a\u7684\u5de5\u7a0b\u4ef7\u6b3e\u7b49\u4e8b\u5b9e\u3002\u7ecf\u8d28\u8bc1\uff0c\u88ab\u544a\u5bf9\u4e0a\u8ff0\u8bc1\u636e\u7684\u771f\u5b9e\u6027\u4e0d\u6301\u5f02\u8bae\uff0c\u63d0\u51fa\u6240\u6b20\u6b3e\u5e94\u4ee5\u88ab\u544a\u51fa\u5177\u7684\u6e05\u7b97\u8bc1\u660e\u4e3a\u51c6\u3002\u672c\u9662\u5bf9\u8be5\u7ec4\u8bc1\u636e\u7684\u771f\u5b9e\u6027\u4e88\u4ee5\u91c7\u4fe1\u3002\b\u8bc1\u636e\u4e8c\u3001\u62cd\u5356\u516c\u544a\u3001\u6267\u884c\u5f02\u8bae\u4e66\u3002\u7528\u4ee5\u8bc1\u660e\u672c\u6848\u8bbc\u4e89\u5de5\u7a0b\u88ab\u62cd\u5356\u53ca\u539f\u544a\u5df2\u63d0\u51fa\u5f02\u8bae\u7684\u4e8b\u5b9e\u3002\u7ecf\u8d28\u8bc1\uff0c\u88ab\u544a\u5bf9\u771f\u5b9e\u6027\u4e0d\u6301\u5f02\u8bae\uff0c\u63d0\u51fa\u4e0e\u672c\u6848\u65e0\u5173\u7684\u5f02\u8bae\u3002\u672c\u9662\u8ba4\u4e3a\uff0c\u8be5\u7ec4\u8bc1\u636e\u4e0e\u672c\u6848\u65e0\u5173\uff0c\u672c\u9662\u4e0d\u4e88\u91c7\u4fe1\u3002\b\u88ab\u544a\u5965\u514b\u6b27\u5e1d\u65af\u79d1\u6280\u516c\u53f8\u5728\u4e3e\u8bc1\u671f\u9650\u5185\u672a\u5411\u6cd5\u5ead\u63d0\u4f9b\u8bc1\u636e\u3002\b\u6839\u636e\u4e0a\u8ff0\u6709\u6548\u8bc1\u636e\u5e76\u7ed3\u5408\u6cd5\u5ead\u8c03\u67e5\u7684\u60c5\u51b5\uff0c\u672c\u9662\u8ba4\u5b9a\u5982\u4e0b\u6848\u4ef6\u4e8b\u5b9e\uff1a\b\u539f\u544a\u6c5f\u82cf\u5f18\u76db\u5efa\u8bbe\u516c\u53f8\u4e8e1998\u5e7411\u670827\u65e5\u5728\u5de5\u5546\u884c\u653f\u7ba1\u7406\u90e8\u95e8\u6ce8\u518c\u6210\u7acb\uff0c\u5176\u7ecf\u8425\u8303\u56f4\u5305\u62ec\uff1a\u627f\u5305\u4e0e\u5176\u5b9e\u529b\u3001\u89c4\u6a21\u3001\u4e1a\u7ee9\u76f8\u9002\u5e94\u7684\u56fd\u5916\u5de5\u7a0b\u9879\u76ee\uff0c\u5bf9\u5916\u6d3e\u9063\u5b9e\u65bd\u4e0a\u8ff0\u5883\u5916\u5de5\u7a0b\u6240\u9700\u7684\u52b3\u52a1\u4eba\u5458\uff0c\u623f\u5c4b\u5efa\u7b51\u5de5\u7a0b\u65bd\u5de5\u603b\u627f\u5305\uff0c\u5efa\u7b51\u88c5\u9970\u88c5\u4fee\u5de5\u7a0b\u4e13\u4e1a\u627f\u5305\uff0c\u5730\u57fa\u4e0e\u57fa\u7840\u5de5\u7a0b\u4e13\u4e1a\u627f\u5305\u7b49\u4e1a\u52a1\u30022010\u5e7412\u670811\u65e5\uff0c\u539f\u3001\u88ab\u544a\u53cc\u65b9\u7b7e\u8ba2\u300a\u5efa\u8bbe\u5de5\u7a0b\u65bd\u5de5\u5408\u540c\u300b\u4e00\u4efd\uff0c\u5408\u540c\u4e3b\u8981\u7ea6\u5b9a\uff0c\u7531\u539f\u544a\u627f\u63a5\u88ab\u544a\u4f4d\u4e8e\u6df1\u5733\u5de5\u4e1a\u56ed\u3001\u5efa\u7b51\u9762\u79ef\u7ea612314\u5e73\u65b9\u7c73\u7684\u7efc\u5408\u697c\u3001\u529e\u516c\u697c\u5efa\u7b51\u5de5\u7a0b\uff1b\u5de5\u7a0b\u5b9e\u884c\u5168\u5305\uff08\u5305\u5de5\u3001\u6599\u3001\u8bbe\u5907\u7b49\uff09\uff0c\u6309\u65bd\u5de5\u84dd\u56fe\u65bd\u5de5\uff1b\u5408\u540c\u5de5\u671f\u603b\u65e5\u5386\u5929\u6570\u4e3a240\u5929\uff0c\u5de5\u7a0b\u8d28\u91cf\u6807\u51c6\u6309\u56fd\u5bb6\u6807\u51c6\u6267\u884c\uff1b\u5408\u540c\u4ef7\u6b3e\u4e3a958\u4e07\u5143\u3002\u5408\u540c\u8fd8\u7ea6\u5b9a\u4e86\u5176\u4ed6\u4e13\u7528\u6761\u6b3e\u3001\u8fdd\u7ea6\u8d23\u4efb\u3001\u4fdd\u4fee\u671f\u9650\u7b49\u5185\u5bb9\u3002\u5408\u540c\u7b7e\u8ba2\u540e\uff0c\u539f\u544a\u5373\u4e8e\u5f53\u5e74\u5e95\u7ec4\u7ec7\u4eba\u5458\u8fdb\u573a\u65bd\u5de5\uff0c2011\u5e746\u67085\u65e5\u5c01\u9876\uff0c\u5728\u6b64\u671f\u95f4\uff0c\u88ab\u544a\u5171\u652f\u4ed8\u5de5\u7a0b\u6b3e\u5171\u8ba1460\u4e07\u5143\u3002\u56e0\u88ab\u544a\u672a\u6309\u8fdb\u5ea6\u652f\u4ed8\u5de5\u7a0b\u6b3e\uff0c\u5de5\u7a0b\u505c\u5de5\uff0c\u539f\u544a\u6d3e\u5458\u4e00\u76f4\u5728\u5de5\u5730\u770b\u7ba1\uff0c\u4e14\u5916\u67b6\u4e5f\u672a\u8fdb\u884c\u62c6\u9664\u30022016\u5e748\u6708\uff0c\u56e0\u88ab\u544a\u4e0e\u4ed6\u4eba\u53d1\u751f\u7ecf\u6d4e\u7ea0\u7eb7\uff0c\u5176\u516c\u53f8\u5728\u4e0a\u8ff0\u6df1\u5733\u5de5\u4e1a\u56ed\u4eab\u6709\u7684\u571f\u5730\u4f7f\u7528\u6743\u88ab\u672c\u9662\u62cd\u5356\uff0c\u5bfc\u81f4\u8be5\u571f\u5730\u4e0a\u7531\u539f\u544a\u627f\u63a5\u7684\u4e0a\u8ff0\u672a\u5b8c\u5de5\u5de5\u7a0b\u4e5f\u88ab\u62cd\u5356\uff0c\u539f\u544a\u5f97\u77e5\u8fd9\u4e00\u6d88\u606f\u540e\uff0c\u5411\u672c\u9662\u63d0\u51fa\u5f02\u8bae\uff0c\u5e76\u4e0e\u88ab\u544a\u8fdb\u884c\u534f\u5546\u7ed3\u7b97\u5de5\u7a0b\u6b3e\u30022016\u5e7411\u670828\u65e5\uff0c\u539f\u544a\u5236\u4f5c\u5965\u514b\u6b27\u5e1d\u65af\u8944\u6a0a\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u529e\u516c\u697c\u3001\u7efc\u5408\u697c\u5de5\u7a0b\u9020\u4ef7\u8ba1\u7b97\u4e66\uff0c\u5f97\u51fa\u7ed3\u8bba\u4e3a\u529e\u516c\u697c\u5df2\u5b8c\u6210\u7684\u5de5\u7a0b\u9020\u4ef7\u4e3a4099994.63\u5143\uff1b\u7efc\u5408\u697c\u5df2\u5b8c\u6210\u7684\u5de5\u7a0b\u9020\u4ef7\u4e3a5427651.27\u5143\u3002\u5f53\u65e5\uff0c\u539f\u544a\u51fa\u5177\u5173\u4e8e\u5965\u514b\u6b27\u5e1d\u65af\u8944\u9633\u8282\u80fd\u79d1\u6280\u6709\u9650\u516c\u53f8\u529e\u516c\u697c\u53ca\u7efc\u5408\u697c\u6240\u6b20\u5de5\u7a0b\u6b3e\u7684\u57fa\u672c\u60